{
  "root": "apps/api",
  "sourceRoot": "apps/api/src",
  "projectType": "application",
  "targets": {
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "composer install",
        "cwd": "apps/api"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "php -S 0.0.0.0:8080",
        "cwd": "apps/api"
      }
    }
  },
  "tags": []
}
